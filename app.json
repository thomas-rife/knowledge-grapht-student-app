{
  "expo": {
    "name": "Knowledge Grapht",
    "slug": "knowledge-grapht",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "knowledge-grapht",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/icons/ios-light.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thomasrife.knowledgegrapht",
      "icon": {
        "dark": "./assets/images/icons/ios-dark.png",
        "light": "./assets/images/icons/ios-light.png"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.thomasrife.knowledgegrapht",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icons/splash-icon-dark.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bc861f28-9f7b-47e7-b541-791a787e16c6"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/bc861f28-9f7b-47e7-b541-791a787e16c6"
    }
  }
}
